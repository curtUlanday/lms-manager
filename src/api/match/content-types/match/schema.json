{
  "kind": "collectionType",
  "collectionName": "matches",
  "info": {
    "singularName": "match",
    "pluralName": "matches",
    "displayName": "Match",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "match_id": {
      "type": "customField",
      "customField": "plugin::strapi-advanced-uuid.uuid",
      "required": true
    },
    "host_id": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "match_status": {
      "type": "enumeration",
      "enum": [
        "Ongoing",
        "Expired",
        "Finished"
      ]
    },
    "rounds": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::round.round",
      "mappedBy": "match_id"
    },
    "player_answer_data": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::player.player"
    },
    "players": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "plugin::users-permissions.user"
    }
  }
}
